model <- nnet::multinom(Species ~ ., data = train) %>%
stepAIC(direction = "forward", trace = FALSE)
summary(model)
model <- nnet::multinom(Species ~ ., data = train) %>%
stepAIC(direction = "backward", trace = FALSE)
summary(model)
pred_classes <- model %>% predict(test)
mean(pred_classes == test$Species)
