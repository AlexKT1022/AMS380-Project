train <- iris[train_idx, ]
test <- iris[-train_idx, ]
model <- nnet::multinom(Species ~ ., data = train)
summary(model)
model <- nnet::multinom(Species ~ ., data = train) %>% stepAIC(trace = FALSE)
summary(model)
model <- nnet::multinom(Species ~ ., data = train) %>%
